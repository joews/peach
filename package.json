{
  "name": "peach-lang",
  "version": "0.4.1",
  "description": "A tiny functional language",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "prepublish": "npm run dist",
    "lint": "standard",
    "dist": "mkdir -p dist && cp -r src/peach.pegjs src/__tests__/fixtures dist && tsc && chmod u+x ./dist/bin/peach.js",
    "clean": "rm -rf dist"
  },
  "bin": {
    "peach": "./dist/bin/peach.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwhitfieldseed/peach.git"
  },
  "keywords": [
    "peach",
    "language",
    "lisp"
  ],
  "author": "Joe Whitfield-Seed <joeseed86@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwhitfieldseed/peach/issues"
  },
  "homepage": "https://github.com/jwhitfieldseed/peach#readme",
  "dependencies": {
    "minimist": "^1.2.0",
    "pegjs": "^0.10.0"
  },
  "devDependencies": {
    "@types/jest": "^18.1.1",
    "@types/minimist": "^1.2.0",
    "@types/node": "^7.0.5",
    "@types/pegjs": "^0.10.0",
    "chalkline": "0.0.5",
    "jest": "^19.0.2",
    "standard": "^8.3.0",
    "typescript": "^2.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/src/__tests__/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*tests.ts"
    ]
  }
}
